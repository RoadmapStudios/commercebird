{
  "name": "wooventory/wooventory",
  "description": "This plugin helps you get the most of Wooventory by allowing you to manage product images, integrations and more",
  "type": "wordpress-plugin",
  "license": "GPL-3.0-or-later",
  "autoload": {
    "psr-4": {
      "RMS\\Admin\\": "admin/includes/"
    },
    "classmap": [
      "includes/classes/"
    ]
  },
  "authors": [
    {
      "name": "Roadmap Studios",
      "email": "info@wooventory.com",
      "role": "Owner"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "ext-curl": "*",
    "automattic/woocommerce": ">=3.1.0"
  },
  "require-dev": {
    "filp/whoops": ">=2.15.4",
    "roots/wordpress-no-content": ">=6.3.2",
    "symfony/var-dumper": "^5.4",
    "wp-coding-standards/wpcs": ">=3.0.1"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
